/*
* Programming Quiz: Facebook Friends (7-5)
*/

// your code goes here

var facebookProfile = {
  name: "Jhon",
  friends: 1000,
  messages: ["hello world", "Poptarts", "Corgi"],
  postMessage: function(message) {
    facebookProfile.messages.push("new message");
  },
  deleteMessage: function(index){
      facebookProfile.messages.splice(index, 1);
  },
  addFriend: function(){
    facebookProfile.friends++;
  },
  removeFriend: function(){
    facebookProfile.friends--;
  }
}; //closes object facebookProfile
