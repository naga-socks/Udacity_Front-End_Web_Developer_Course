/*
 * Programming Quiz: JuliaJames (4-1)
 */

var x = 1;

while (x <= 20) {
    // check divisibility
    var check = (x%3 === 0 && x%5 === 0) ? "JuliaJames" : (x%3 === 0 ? "Julia" : x%5 === 0 ? "James" : x);
    // print Julia, James, or JuliaJames
    console.log(check);
    // increment x
    x = x+1;
}
